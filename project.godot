config_version=5

[application]

config/name="Stellar Cartographer"
config/version="0.4.1.0"
run/main_scene="res://Scenes/Game Master/gameMaster.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://Graphics/fallback_icon.png"
config/macos_native_icon="res://Graphics/icon.icns"
config/windows_native_icon="res://Graphics/icon.ico"

[autoload]

global_data="*res://Data/global_data.gd"
game_data="*res://Data/game_data.gd"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=1600
window/size/viewport_height=900
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

export/convert_text_resources_to_binary=false

